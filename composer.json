{
    "name": "vpankeev/filament-spatie-translatable",
    "description": "This package allows making translatable your laravel models",
    "license": "MIT",
    "keywords": [
        "laravel-translatable",
        "translate",
        "eloquent",
        "model",
        "i8n",
        "multilingual"
    ],
    "authors": [
        {
            "name": "Vlad",
            "email": "v.pankeev95@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.1",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "laravel/framework": "9.19",
        "spatie/laravel-translatable": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Randes\\Translatable\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Randes\\Translatable\\FilamentSpatieTranslatableProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
